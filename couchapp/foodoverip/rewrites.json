[
    {
        "from": "/",
        "to": "index.html"
    },

    {
        "from": "/food/:id",
        "to": "_show/tweet/:id"
    },

    {
        "from": "/photo/:id",
        "to": "../../:id/photo.png"
    },

    {
        "from": "/random",
        "to": "_random/random",
        "query": {
            "filter": "foodoverip/food"
        }
    },
    {
        "from": "/_changes",
        "to": "../../_changes"
    },
    {
        "from": "/_changes/*",
        "to": "../../_changes/*"
    },

    {
        "from": "/*",
        "to": "*"
    },

    {
        "from": "*",
        "to": "*"
    }

]
