[
    {
        "from": "/",
        "to": "index.html"
    },

        {
        "from": "/photo/:id",
        "to": "../../:id/photo.png"
    },

    {
        "from": "/random",
        "to": "_random/random",
        "query": {
            "filter": "foodoverip/food"
        }
    },
    {
        "from": "/_changes",
        "to": "../../_changes"
    },
    {
        "from": "/_changes/*",
        "to": "../../_changes/*"
    },


    {
        "from": "/static/*",
        "to": "*"
    },

    {
        "from": "/:id",
        "to": "_show/tweet/:id"
    }
]
