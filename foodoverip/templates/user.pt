<html metal:use-macro="base.macros['base']">
    <div metal:fill-slot='content'>
        <img class="fleft avatar" src="${request.static_url('foodoverip:/static/avatars/')}${username}" /><p class="fleft"><a href="http://twitter.com/${username}">@${username}</a> has ${tweet_len} pictures listed here</p>
        <div tal:repeat="id tweet_ids">
            <a href="${request.route_url('get', key=id)}">
                <img class="stacked thumbnail" src="${request.route_url('thumb', key=id)}" />
            </a>
        </div>
    </div>
</html>

